select pload status;
--disable_query_log
--replace_result $srcdir <srcdir>
--eval pload $srcdir/base/t/lm1.lua
--enable_query_log
connection conn1;
select 'first' as info;
select pload status;
connect (conn2,127.0.0.1,$MYSQL_USER,$MYSQL_PASSWORD,test,$PROXY_PORT,);
connection conn2;
select 'second' as info;
select pload status;
disconnect conn2;
connect (conn2,127.0.0.1,$MYSQL_USER,$MYSQL_PASSWORD,test,$PROXY_PORT,);
connection conn2;
select sleep(0.2);
select 'third' as info;
select pload status;
connection default;
disconnect conn1;
disconnect conn2;
